{
    "name": "angular-project",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --proxy-config proxy.conf.json",
        "build-prod": "ng build --prod",
        "build-dev": "ng build",
        "build-${env}": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "generate:mydy": "openapi-generator-cli help",
        "generate:help": "openapi-generator-cli config-help -g typescript-angular",
        "generate:providers-api": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-providers/openapi/rudi-providers-api.json -o src/app/providers/api-providers -c openapitools.json -t ./template",
        "generate:providers-model": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-providers/openapi/rudi-providers-model.json -o src/app/providers/providers-model",
        "generate:acl-api": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-acl/openapi/rudi-acl-api.json -o src/app/acl/acl-api -t ./template",
        "generate:acl-model": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-acl/openapi/rudi-acl-model.json -o src/app/acl/acl-model",
        "generate:konsult": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-konsult/openapi/rudi-konsult-api.json -o src/app/api-konsult -c openapitools.json -t ./template",
        "generate:kaccess": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-facet/rudi-facet-kaccess/src/main/resources/openapi/rudi-kaccess-model.yml -o src/app/api-kaccess",
        "generate:kmedia": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-facet/rudi-facet-kmedia/src/main/resources/openapi/rudi-kmedia-model.yml -o src/app/api-kmedia",
        "generate:kos-api": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-kos/openapi/rudi-kos-api.json -o src/app/kos/kos-api -t ./template",
        "generate:kos-model": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-kos/openapi/rudi-kos-model.json -o src/app/kos/kos-model",
        "generate:all": "npm run generate:kmedia && npm run generate:providers-model && npm run generate:providers-api && npm run generate:kaccess && npm run generate:konsult && npm run generate:acl-model && npm run generate:acl-api && npm run generate:kos-model && npm run generate:kos-api"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.0.0",
        "@angular/cdk": "^11.2.13",
        "@angular/common": "~11.0.0",
        "@angular/compiler": "~11.0.0",
        "@angular/core": "~11.0.0",
        "@angular/flex-layout": "^11.0.0-beta.33",
        "@angular/forms": "~11.0.0",
        "@angular/material": "^11.2.13",
        "@angular/platform-browser": "~11.0.0",
        "@angular/platform-browser-dynamic": "~11.0.0",
        "@angular/router": "~11.0.0",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "angular-svg-icon": "^11.0.0",
        "bootstrap": "^4.5.3",
        "file-saver": "^2.0.5",
        "font-awesome": "^4.7.0",
        "jquery": "^3.5.1",
        "mime": "^2.5.2",
        "moment": "^2.29.1",
        "ngx-bootstrap-navbar": "0.0.7",
        "ngx-filter-pipe": "^2.1.2",
        "ngx-horizontal-scroll-menu": "^1.0.11",
        "ngx-line-truncation": "^1.7.0",
        "ngx-pagination": "^5.0.0",
        "ol": "^6.6.1",
        "query-string": "^6.14.0",
        "rxjs": "^6.6.3",
        "sass": "^1.42.1",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1100.0",
        "@angular/cli": "~11.0.0",
        "@angular/compiler-cli": "~11.0.0",
        "@openapitools/openapi-generator-cli": "2.1.18",
        "@types/jasmine": "~3.6.0",
        "@types/jquery": "^3.5.5",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-openapi-gen": "^0.12.1",
        "node-sass-install": "^1.0.2",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.2"
    }
}
