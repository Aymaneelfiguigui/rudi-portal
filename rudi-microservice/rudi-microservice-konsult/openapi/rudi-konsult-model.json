{
	"openapi": "3.0.0",
	"info": {
		"description": "API Public Konsult Rudi",
		"version": "0.0.1",
		"title": "rudi-projekt-api",
		"termsOfService": "https://swagger.io/terms/",
		"contact": {
			"email": "rudi@datarennes.fr"
		},
		"license": {
			"name": "Apache 2.0",
			"url": "https://www.apache.org/licenses/LICENSE-2.0.html"
		}
	},
	"servers": [
		{
			"url": "/{basePath}",
			"description": "Rudi Konsult API",
			"variables": {
				"basePath": {
					"default": "konsult/v1"
				}
			}
		}
	],
	"paths": {
	},
	"components": {
		"schemas": {
			"LayerInformation": {
				"type": "object",
				"properties": {
					"label": {
						"type": "string"
					},
					"url": {
						"type": "string"
					},
					"layerName": {
						"type": "string"
					},
					"attributions": {
						"type": "string"
					},
					"type": {
						"type": "string"
					},
					"srs": {
						"type": "string"
					},
					"minZoom": {
						"type": "number"
					},
					"maxZoom": {
						"type": "number"
					},
					"format": {
						"type": "string"
					},
					"matrixSet": {
						"type": "string"
					},
					"matrixIdPrefix": {
						"type": "string"
					}
				}
			},
			"EpsgIoResponse": {
				"type": "object",
				"properties": {
					"number_results": {
						"type": "number"
					},
					"results": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/EpsgIoProjection"
						}
					},
					"status": {
						"type": "string"
					}
				}
			},
			"EpsgIoProjection": {
				"type": "object",
				"properties": {
					"code": {
						"type": "string"
					},
					"bbox": {
						"type": "array",
						"items": {
							"type": "number"
						}
					},
					"proj4": {
						"type": "string"
					}
				}
			},
			"Proj4Information":  {
				"description": "Informations nécessaires pour récupérer les informations sur une projection (genre EPSG:3948)",
				"type": "object",
				"properties": {
					"code": {
						"description": "le code EPSG",
						"type": "string"
					},
					"proj4": {
						"description": "la chaîne de caractère qui représente la projection proj4 à ajouter dans OpenLayers",
						"type": "string"
					},
					"bbox": {
						"$ref": "#/components/schemas/Bbox"
					}
				}
			},
			"Bbox": {
				"description": "Représente une BBOX (Bounding box) ou boîte englobante. Il s'agit de deux points dans l'espace qui permettent de définir un rectangle, les BBOX sont normées pour utiliser des points dans le système de coordonnées EPSG:4326 WGS84 ou les coordonnées GPS longitude et latitude",
				"type": "object",
				"properties": {
					"west_longitude": {
						"type": "number"
					},
					"south_latitude": {
						"type": "number"
					},
					"east_longitude": {
						"type": "number"
					},
					"north_latitude": {
						"type": "number"
					}
				}
			},
			"HeroDescription": {
				"description": "Element configurable du Hero de la page d'accueil",
				"type": "object",
				"properties": {
					"left_image": {
						"type": "string"
					},
					"right_image": {
						"type": "string"
					},
					"title1": {
						"type": "string"
					},
					"title2": {
						"type": "string"
					}
				}
			},
			"ProjectsDescription": {
				"description": "Element configurable de la section projects de la page d'accueil",
				"type": "object",
				"properties": {
					"title1": {
						"type": "string"
					},
					"title2": {
						"type": "string"
					},
					"subtitle": {
						"type": "string"
					},
					"description": {
						"type": "string"
					}
				}
			},
			"KeyFigureType": {
				"description": "Type de KeyFigure ",
				"type": "string",
				"enum": [
					"USER",
					"DATASET",
					"ORGANIZATION",
					"PROJEKT"
				]
			},
			"KeyFigure":{
				"description": "Element correspondant à 1 chiffre clé",
				"type": "object",
				"properties": {
					"count": {
						"type": "integer",
						"format": "int64"
					},
					"label": {
						"type": "string"
					},
					"type": {
						"$ref": "#/components/schemas/KeyFigureType"
					}
				}
			},
			"KeyFiguresDescription": {
				"description": "Element de récupération des chiffres clé",
				"type": "object",
				"properties": {
					"keyFiguresLogo": {
						"type": "string"
					},
					"keyFigures": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/KeyFigure"
						}
					}
				}
			},
			"CustomizationDescription": {
				"description": "Element configurable de l'application",
				"type": "object",
				"properties": {
					"override_css_file": {
						"type": "string"
					},
					"main_logo": {
						"type": "string"
					},
					"hero_description": {
						"$ref": "#/components/schemas/HeroDescription"
					},
					"projects_description": {
						"$ref": "#/components/schemas/ProjectsDescription"
					},
					"key_figures_description": {
						"$ref": "#/components/schemas/KeyFiguresDescription"
					}
				}
			}
		}
	}
}
